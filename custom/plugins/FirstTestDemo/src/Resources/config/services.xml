<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="FirstTestDemo\Core\Content\TestDemo\TestDemoDefinition">
            <tag name="shopware.entity.definition" entity="first_test_demo"/>
        </service>

        <service id="FirstTestDemo\Core\Content\TestDemo\Aggregate\TestDemoTranslation\TestDemoTranslationDefinition">
            <tag name="shopware.entity.definition" entity="first_test_demo" />
        </service>

        <service id="FirstTestDemo\Subscriber\ProductSubscriber">
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="FirstTestDemo\Core\Content\Extension\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="FirstTestDemo\Core\Content\Extension\CountryExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="FirstTestDemo\Core\Content\Extension\StateExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="FirstTestDemo\Core\Content\Extension\MediaExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="FirstTestDemo\Core\Content\Extension\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>

    </services>
</container>
